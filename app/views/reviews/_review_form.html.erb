<%= form_for [product,review] do |f| %>
    <div>
        <%= f.label :user_name %>
        <%= f.text_field :user %>
    </div>
    <div>
        <%= f.label :stars %>
        <%= f.number_field :stars %>
    </div>
    <div>
        <%= f.label :review %>
        <%= f.text_area :message %>
    </div>
    <%= f.submit %>
<% end %>